    <decisions>
        <name>Emails_Blocked</name>
        <label>Emails Blocked?</label>
        <locationX>974</locationX>
        <locationY>288</locationY>
        <defaultConnector>
            <targetReference>Send_Trustpilot_Invite</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not Blocked</defaultConnectorLabel>
        <rules>
            <name>Blocked</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Block_emails__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <label>Blocked</label>
        </rules>
    </decisions>
    <decisions>
        <name>Hardcopy_Proof_in_Office</name>
