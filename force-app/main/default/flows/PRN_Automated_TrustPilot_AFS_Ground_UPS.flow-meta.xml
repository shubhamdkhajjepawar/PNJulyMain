    <decisions>
        <name>Where_is_Order_Shipping_From</name>
        <label>Where is Order Shipping From?</label>
        <locationX>248</locationX>
        <locationY>288</locationY>
        <defaultConnector>
            <targetReference>Small_Parcel_or_LTL</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>China</defaultConnectorLabel>
        <rules>
            <name>US</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Shipping_Status_Standard__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>In-Transit (Domestic)</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Send_Domestic_Tracking_Email</targetReference>
            </connector>
            <label>US</label>
        </rules>
    </decisions>
    <description>Adjusted start conditions to include orders with ground shipping company set as TForce.</description>
    <environments>Default</environments>
    <interviewLabel>PRN Automated TrustPilot AFS Ground (UPS) {!$Flow.CurrentDateTime}</interviewLabel>
    <label>PRN Automated TrustPilot AFS Ground (UPS)</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <start>
        <locationX>749</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Where_is_Order_Shipping_From</targetReference>
        </connector>
        <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
        <filterLogic>(1 OR 2) AND (3 OR 7) AND 4 AND 5 AND 6</filterLogic>
        <filters>
            <field>Production_Status__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Off-Press</stringValue>
            </value>
        </filters>
        <filters>
            <field>Production_Status__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>In-Transit</stringValue>
            </value>
        </filters>
        <filters>
            <field>Ground_Shipping_Company__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>UPS Ground</stringValue>
            </value>
        </filters>
        <filters>
            <field>Tracking_No__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue></stringValue>
            </value>
        </filters>
        <filters>
            <field>Block_emails__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>CloseDate</field>
            <operator>GreaterThan</operator>
            <value>
                <dateValue>2013-11-01</dateValue>
            </value>
        </filters>
        <filters>
            <field>Ground_Shipping_Company__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>TForce</stringValue>
            </value>
        </filters>
        <object>Opportunity</object>
        <recordTriggerType>Update</recordTriggerType>
        <scheduledPaths>
            <name>X10_Days_After</name>
            <connector>
                <targetReference>Emails_Blocked</targetReference>
            </connector>
            <label>10 Days After</label>
            <offsetNumber>10</offsetNumber>
            <offsetUnit>Days</offsetUnit>
            <timeSource>RecordTriggerEvent</timeSource>
        </scheduledPaths>
        <scheduledPaths>
            <name>X30_Days_Later</name>
            <connector>
                <targetReference>Hardcopy_Proof_in_Office</targetReference>
            </connector>
            <label>30 Days Later</label>
            <offsetNumber>30</offsetNumber>
            <offsetUnit>Days</offsetUnit>
            <timeSource>RecordTriggerEvent</timeSource>
        </scheduledPaths>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
</Flow>
