    <rules>
        <fullName>Metallic ink and Linen Texture%2E Check for small type%2E</fullName>
        <active>false</active>
        <booleanFilter>1 AND 2 AND 3 AND 4 AND 5</booleanFilter>
        <criteriaItems>
            <field>Game_Components__c.Metallic_Ink_Color__c</field>
            <operation>equals</operation>
            <value>Gold,Silver,Gold and Silver</value>
        </criteriaItems>
        <criteriaItems>
            <field>Game_Components__c.Packaging_Metallic_Ink_Color__c</field>
            <operation>equals</operation>
            <value>Gold,Silver,Gold and Silver</value>
        </criteriaItems>
        <criteriaItems>
            <field>Game_Components__c.Packaging_Specialty_Options__c</field>
            <operation>includes</operation>
            <value>Linen Texture</value>
        </criteriaItems>
        <criteriaItems>
            <field>Game_Components__c.Specialty_Options_Card__c</field>
            <operation>includes</operation>
            <value>Linen Texture</value>
        </criteriaItems>
        <criteriaItems>
            <field>Custom_Game_Quote__c.Proofing__c</field>
            <operation>equals</operation>
            <value>Electronic,Hard Copy</value>
        </criteriaItems>
        <description>Metallic ink and Linen are used together. Check the usage on the text. Metallic ink on small text and linen texture creates light distortions that hurt text.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
