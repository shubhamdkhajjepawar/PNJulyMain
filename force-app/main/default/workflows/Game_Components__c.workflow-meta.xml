        <field>Add_On_Markup_1__c</field>
        <formula>IF(AND(Add_On_Cost_1__c &gt; 0,Add_On_Cost_1__c &lt; 1000), 1, 
IF(AND(Add_On_Cost_1__c &gt;= 1000,Add_On_Cost_1__c &lt; 2500), 0.80, 
IF(AND(Add_On_Cost_1__c &gt;= 2500,Add_On_Cost_1__c &lt; 3000), 0.70, 
IF(AND(Add_On_Cost_1__c &gt;= 3000,Add_On_Cost_1__c &lt; 12500), 0.60, 
IF(Add_On_Cost_1__c &gt;= 12500, 0.50,0 
)))))</formula>
